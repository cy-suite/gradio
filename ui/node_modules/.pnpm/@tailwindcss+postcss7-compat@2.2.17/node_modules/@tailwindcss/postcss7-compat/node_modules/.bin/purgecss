#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/purgecss@4.1.3/node_modules/purgecss/bin/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/purgecss@4.1.3/node_modules/purgecss/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/purgecss@4.1.3/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules:/Users/peterallen/Projects/gradio/node_modules:/Users/peterallen/Projects/node_modules:/Users/peterallen/node_modules:/Users/node_modules:/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/@tailwindcss+postcss7-compat@2.2.17/node_modules/@tailwindcss/postcss7-compat/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/@tailwindcss+postcss7-compat@2.2.17/node_modules/@tailwindcss/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/@tailwindcss+postcss7-compat@2.2.17/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/purgecss@4.1.3/node_modules/purgecss/bin/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/purgecss@4.1.3/node_modules/purgecss/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/purgecss@4.1.3/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules:/Users/peterallen/Projects/gradio/node_modules:/Users/peterallen/Projects/node_modules:/Users/peterallen/node_modules:/Users/node_modules:/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/@tailwindcss+postcss7-compat@2.2.17/node_modules/@tailwindcss/postcss7-compat/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/@tailwindcss+postcss7-compat@2.2.17/node_modules/@tailwindcss/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/@tailwindcss+postcss7-compat@2.2.17/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../purgecss@4.1.3/node_modules/purgecss/bin/purgecss.js" "$@"
else
  exec node  "$basedir/../../../../../../purgecss@4.1.3/node_modules/purgecss/bin/purgecss.js" "$@"
fi
