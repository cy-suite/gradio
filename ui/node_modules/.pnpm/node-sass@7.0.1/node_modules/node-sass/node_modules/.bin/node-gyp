#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node-gyp@8.4.1/node_modules/node-gyp/bin/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node-gyp@8.4.1/node_modules/node-gyp/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node-gyp@8.4.1/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules:/Users/peterallen/Projects/gradio/node_modules:/Users/peterallen/Projects/node_modules:/Users/peterallen/node_modules:/Users/node_modules:/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node-sass@7.0.1/node_modules/node-sass/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node-sass@7.0.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node-gyp@8.4.1/node_modules/node-gyp/bin/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node-gyp@8.4.1/node_modules/node-gyp/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node-gyp@8.4.1/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules:/Users/peterallen/Projects/gradio/node_modules:/Users/peterallen/Projects/node_modules:/Users/peterallen/node_modules:/Users/node_modules:/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node-sass@7.0.1/node_modules/node-sass/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node-sass@7.0.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node-gyp@8.4.1/node_modules/node-gyp/bin/node-gyp.js" "$@"
else
  exec node  "$basedir/../../../../../node-gyp@8.4.1/node_modules/node-gyp/bin/node-gyp.js" "$@"
fi
